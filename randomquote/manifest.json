{
  "name": "Random Quotes",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Generates random quotes",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
